{
  "name": "Hagrid",
  "sources": {
    "dir": "src",
    "subdirs": true
  },
  "bs-dependencies": [
    "bs-reform",
    "reason-react",
    "reason-apollo",
    "reason-apollo-hooks",
    "@reasonml-community/graphql-ppx",
    "reschema"
  ],
  "reason": {
    "react-jsx": 3
  },
  "bsc-flags": [
    "-bs-super-errors",
    "-open Belt"
  ],
  "ppx-flags": [
    "@baransu/graphql_ppx_re/ppx"
  ],
  "refmt": 3,
  "package-specs": {
    "module": "commonjs",
    "in-source": true
  },
  "warnings": {
    "error": "+8"
  },
  "suffix": ".bs.js"
}
